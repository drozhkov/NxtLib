{
  "version": "1.1.18",
  "description": "NxtLib Class Library",
  "authors": [ "Liberty Swede" ],
  "title": "NxtLib",
  "summary": "NET Framework API for the NXT platform",
  "tags": [ "nxt", "bitcoin", "cryptocurrency" ],
  "projectUrl": "https://github.com/libertyswede/NxtLib",
  "releaseNotes": "NxtLib is a typed wrapper for the Json API in the Nxt platform ( http://nxt.org/ ) It currently supports NXT version 1.9.2",

  "dependencies": {
    "Newtonsoft.Json": "8.0.3"
  },

  "frameworks": {
    "dotnet": {
      "dependencies": {
        "System.Collections": "4.0.10",
        "System.Linq": "4.0.0",
        "System.Threading": "4.0.10",
        "System.Runtime": "4.0.20",
        "Microsoft.CSharp": "4.0.0",
        "System.IO.Compression": "4.0.0",
        "System.Net.Http": "4.0.0",
        "System.Runtime.Numerics": "4.0.0",
        "System.Security.Cryptography.Algorithms": "4.0.0-beta-23409"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Net.Http": "",
        "System.Numerics": ""
      }
    },
    "net40": {
      "dependencies": {
        "Microsoft.Bcl.Async": "1.0.168",
        "Microsoft.Net.Http": "2.2.29"
      },
      "frameworkAssemblies": {
        "System.Numerics": ""
      }
    }
  }
}
