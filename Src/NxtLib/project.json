{
  "version": "1.1.25",
  "description": "NxtLib Class Library",
  "authors": [ "Liberty Swede" ],
  "packOptions": {
    "summary": "NET Framework API for the NXT platform",
    "tags": [ "nxt", "ardor", "bitcoin", "cryptocurrency" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/libertyswede/NxtLib.git"
    },
    "releaseNotes": "NxtLib is a typed wrapper for the Json API in the Nxt platform ( http://nxt.org/ ) It currently supports NXT version 1.10.2",
    "requireLicenseAcceptance": false
  },

  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },

  "frameworks": {
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NETSTANDARD13" ]
      },
      "dependencies": {
        "System.Net.Http": "4.1.0",
        "System.IO.Compression": "4.1.0",
        "System.Runtime.Numerics": "4.0.1",
        "System.Reflection.TypeExtensions": "4.1.0"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Net.Http": "",
        "System.Numerics": ""
      }
    },
    "net40": {
      "dependencies": {
        "Microsoft.Bcl.Async": "1.0.168",
        "Microsoft.Net.Http": "2.2.29"
      },
      "frameworkAssemblies": {
        "System.Numerics": ""
      }
    }
  }
}
